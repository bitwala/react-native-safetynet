  "dependencies": {
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/node": "14.14.20",
    "@types/node-fetch": "2.5.7",
    "@types/react-native": "0.63.45",
    "husky": "4.3.7",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "typescript": "4.1.3"

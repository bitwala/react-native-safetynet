{
  "name": "@bitwala/react-native-safetynet",
  "version": "0.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/bitwala/react-native-safetynet.git",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
